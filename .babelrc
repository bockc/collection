{
    "presets": ["module:metro-react-native-babel-preset"],
    "plugins": [
      [
        "module-resolver",
        {
          "alias": {
            "root": "."
          }
        }
      ],
      "transform-class-properties",
      "@babel/plugin-proposal-optional-chaining"
    ],
    "env": {
      "production": {
        "plugins": ["transform-remove-console"]
      }
    }
  }